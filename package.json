{
  "name": "vue-openid-oauth2-google",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "node --experimental-specifier-resolution=node --loader ts-node/esm -r dotenv/config server.local.ts dotenv_config_path=.env.development",
    "build": "run-p build:client build:server",
    "build:client": "run-p type:check build:vite",
    "build:server": "node ./build.js sit",
    "build:vite": "vite build --mode sit",
    "test:e2e": "playwright test",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.6",
    "@types/express": "^4.17.21",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "crypto-es": "^2.1.0",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "jose": "^5.2.0",
    "openid-client": "^5.6.4",
    "ts-node": "^10.9.2",
    "universal-base64": "^2.1.0",
    "vue": "^3.3.11",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.1",
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node18": "^18.2.2",
    "@types/node": "^18.19.10",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.0",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "npm-run-all2": "^6.1.1",
    "postcss": "^8.4.33",
    "tailwindcss": "^3.4.1",
    "typescript": "~5.3.0",
    "vite": "^5.0.10",
    "vue-tsc": "^1.8.25"
  }
}
